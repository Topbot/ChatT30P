<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <!-- WebDeploy publish profile for Visual Studio / MSDeploy
         Replace the placeholder values or create a corresponding .pubxml.user
         with credentials (this file intentionally does NOT contain passwords).
    -->
    <PublishProtocol>MSDeploy</PublishProtocol>
    <PublishUrl>your-server.example.com:8172/msdeploy.axd</PublishUrl>
    <PublishProvider>MSDeploy</PublishProvider>
    <UserName>your-username</UserName>
    <SiteName>Default Web Site/YourApp</SiteName>
    <DeployIisAppPath>Default Web Site/YourApp</DeployIisAppPath>
    <AllowUntrustedCertificate>True</AllowUntrustedCertificate>
    <_SavePWD>False</_SavePWD>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
  </PropertyGroup>
</Project>
